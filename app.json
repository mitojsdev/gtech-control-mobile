{
  "expo": {
    "name": "GtechControlMobile",
    "slug": "GtechControlMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/gtech_logo_app.png",
    "splash": {
      "image": "./assets/images/splash_gtech.png",
      "resizeMode": "contain",
      "backgroundColor": "#00C853"
    },
    "scheme": "gtechcontrolmobile",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/gtech_logo_app.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.juniodev.GtechControlMobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "52bd7a8d-e763-4c96-9f91-af64ff66e9c8"
      }
    }
  }
}
